    <script type="text/javascript">

        // Sidebar links active state
        var path = window.location.href; // because the 'href' property of the DOM element is the absolute path
        $('#logo-sidebar a').each(function () {
            let cssActive = "link-active";
            if (this.href === path) {
                $(this).addClass(cssActive);
                // add to inner svg
                $(this).find('svg').addClass(cssActive);

            }
        });
        function pageLoad() {
                initFlowbite();
