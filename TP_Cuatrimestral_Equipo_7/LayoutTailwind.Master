    <style type="text/tailwindcss">
        @layer utilities {

            .link-active {
                @apply text-blue-500 !important;
            }

            .status-pill {
                /*@apply inline-block py-2  px-4 rounded-full text-sm;*/
                @apply min-w-24 justify-center me-2 mt-1.5 inline-flex items-center rounded px-2.5 py-1 text-xs font-medium;
            }

            .status-pill--pending {
                @apply bg-yellow-200 text-yellow-800 dark:bg-yellow-900 dark:text-yellow-100;
            }

            .status-pill--doing {
                @apply bg-orange-200 text-orange-800 dark:bg-orange-900 dark:text-orange-100;
            }

            .status-pill--done {
                @apply bg-purple-200 text-purple-800 dark:bg-purple-900 dark:text-purple-100;
            }

            .status-pill--delivered {
                @apply bg-green-200 text-green-800 dark:bg-green-900 dark:text-green-100;
            }

            .status-pill--canceled {
                @apply bg-red-200 text-red-800 dark:bg-red-900 dark:text-red-100;
            }

            .chzn-container-single, .chzn-single, .chzn-drop, .chzn-container-single .chzn-search input, .chzn-container-single .chzn-results .no-results, .chzn-results, .chzn-container-single, .chzn-single {
                @apply bg-gray-200 text-gray-900 focus:ring-blue-500 focus:border-blue-500 block dark:bg-gray-700 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500 !important;
            }
           
         select:not([size]):disabled {
             @apply bg-none pl-2 text-white bg-gray-800 !important;
         }
         
         .label-default{
             @apply block mb-2 text-sm font-medium text-gray-900 dark:text-white;
         }
        .input-default{
            @apply bg-gray-50 h-[24px] border border-gray-300 text-gray-900 text-sm focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500;
        }
         
         .label-error{
             @apply block mb-2 text-sm font-medium text-red-700 dark:text-red-500 !important;
         }
         
         .input-error{
         @apply bg-gray-700 h-[24px] border border-red-500 text-white text-sm focus:ring-red-500 focus:border-blue-500 block w-full p-2.5 !important;
         }
         .label-success{
             @apply block mb-2 text-sm font-medium text-green-700 dark:text-green-500 !important;
         }
        .input-success{
            @apply bg-green-50 border border-green-500 text-green-900 dark:text-green-400 placeholder-green-700 dark:placeholder-green-500 text-sm rounded-lg focus:ring-green-500 focus:border-green-500 block w-full p-2.5 dark:bg-gray-700 dark:border-green-500 !important;
        }
        }
      </style>
    
